package screenshots;

import java.util.ArrayList;

public class Department {
	
	
	public static void main(String[] args) {	
		
	}
	
	
	public String getdepartment(String dep1)
	{
		
		ArrayList<String> department = new ArrayList<String>();
		department.add("sales");
		department.add("manufacturing");
		department.add("inventory");
		department.add("customer");
		department.add("admin");
		
		String dep =" ";

		if(department.contains(dep1))
			
			dep = dep1;
		return dep;
	}

}
