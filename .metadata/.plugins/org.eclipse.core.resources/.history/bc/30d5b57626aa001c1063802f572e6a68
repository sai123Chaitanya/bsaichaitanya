
public class StudentMain {
	
	private String Name;
	private Integer marks;
	private Integer PASS_THRESHOLD;
	
	
	
	
	
	
	public StudentMain(String Name,Integer marks) {
		
		this.Name = Name;
		this.marks = marks;
		this.PASS_THRESHOLD =35;
	
	}
	
	
	
	public void setName(String Name)
	{
		this.Name = Name; 	
			
	}
	
	public String getName() { return Name;
		
	}
	
	public void setMarks(Integer marks) { this.marks = marks;
		
	}
	
	public Integer getMarks() {
		return marks;
	}
	
	public Integer getpassthresholdmarks() {
		
		return PASS_THRESHOLD;
	}
	
	@Override
	public String toString()
	{
		
		
		return String.format("%s   %s", getName(),getMarks());
		
	}
	
	

}
