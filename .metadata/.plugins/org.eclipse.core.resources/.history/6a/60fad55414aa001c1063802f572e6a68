package screenshots;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.TreeMap;

public class MapExamples2 {
	
public static void main(String[] args) {
	
	
	
	Department dobj = new Department();
	
	
	

	HashMap<String, ArrayList<?>> ab = new HashMap<String, ArrayList<?>>();
	ab.put(dobj.getdepartment("sales"), dobj.get_sales_employeeslist());
	ab.put(dobj.getdepartment("manufacturing"),dobj.get_manufacturing_employees());
	ab.put(dobj.getdepartment("inventory"), dobj.get_inventory_employees());
	ab.put(dobj.getdepartment("customer"), dobj.get_customer_employees());
	ab.put(dobj.getdepartment("admin"),dobj.get_admin_employees());
	

}	

}
