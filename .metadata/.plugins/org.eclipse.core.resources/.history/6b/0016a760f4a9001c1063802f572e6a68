
public class Pizzaprice {
	
	
	Toppings topping1;
	Toppings topping2;
	Toppings topping3;
	Toppings topping4;
	Pizza pizza;
	
	
	public Pizzaprice(Pizza pizza,Toppings topping1)
	{
		this.topping1 = topping1;
		this.pizza = pizza;
		
	}
	public Pizzaprice(Pizza pizza,Toppings topping1,Toppings topping2)
	{
		this.topping1 = topping1;
		this.topping2 = topping2;
		
		this.pizza = pizza;
		
	}
	public Pizzaprice(Pizza pizza,Toppings topping1,Toppings topping2,Toppings topping3)
	{
		this.topping1 = topping1;
		this.topping2 = topping2;
		this.topping3 = topping3;
		this.topping4 = topping4;
		this.pizza = pizza;
		
	}
	public Pizzaprice(Pizza pizza,Toppings topping)
	{
		this.topping = topping;
		this.pizza = pizza;
		
	}
	public Pizzaprice(Pizza pizza,Toppings topping)
	{
		this.topping = topping;
		this.pizza = pizza;
		
	}
	
	public  Toppings gettoppings()
	{
		
		
	return topping;
	
	}
	
	public Pizza getpizza()
	{
		
		return pizza;
	}
	
	public String toString()
	{
		return pizza + "   of  " + topping;
		
	}

}
